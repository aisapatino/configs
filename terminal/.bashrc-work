# Work/mac-specific bash config

# Base
#------

source ~/Projects/configs/terminal/.bashrc-base

# Colors
#--------

# Differs from linux. See man ls for more info
export LSCOLORS=ExGxBxDxCxEgEdxbxgExEx

# Autocomplete
#--------------

# Enhanced bash autocomplete. See http://superuser.com/a/288491
if [ -f `brew --prefix`/etc/bash_completion ]; then
  source `brew --prefix`/etc/bash_completion
fi

# Untracked .bashrc (for passwords, etc.)
#-----------------------------------------

if [ -f ~/Projects/flabs/.bashrc ] ; then
  source ~/Projects/flabs/.bashrc
fi


# django (for pylint)
export PYTHONPATH=$PYTHONPATH:~/Projects/personal/sjfnw/libs
