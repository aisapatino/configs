# Base
#------

source ~/Projects/configs/terminal/.bashrc-base

# Path
#------

export PATH=$PATH:$HOME/Devel/google_appengine

if [ -d "$HOME/.node" ] ; then
  export PATH=$PATH:$HOME/.node/bin:$HOME/.node/lib/node_modules
fi

# Pythonpath
#------------

# gae
export PYTHONPATH=$PYTHONPATH:$HOME/Devel/google_appengine
export PYTHONPATH=$PYTHONPATH:$HOME/Devel/google_appengine/lib/webob-1.2.3
# misc installed tools
export PYTHONPATH=$PYTHONPATH:$HOME/Devel
# django for pylint
export PYTHONPATH=$PYTHONPATH:$HOME/Projects/sjfnw/libs

# Aliases
#---------

alias cloudsql="mysql --host=173.194.109.221 -uroot -p"

# Google Cloud SDK
#------------------

if [ -d $HOME/Devel/google-cloud-sdk ] ; then
  source $HOME/Devel/google-cloud-sdk/path.bash.inc
  source $HOME/Devel/google-cloud-sdk/completion.bash.inc
fi

# Misc
#-------

# selenium
export CHROMEDRIVER="$HOME/Devel/chromedriver"
