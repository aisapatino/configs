[advice]
  statusHints = false
[blame]
  date = short
[color]
  ui = always
[core]
  autocrlf = false
  excludesfile = ~/.gitignore
  editor = vim
[credential]
  helper = cache --timeout=3600
[diff]
  renames = true
  tool = gvimdiff
[difftool]
  prompt = false
[fetch]
  prune = true
[hub]
  host = gecgithub01.walmart.com
[merge]
  tool = fugitive
[mergetool]
  prompt = false
  keepBackup = false
[mergetool "fugitive"]
  cmd = gvim -f -c \"Gdiff\" \"$MERGED\"
[push]
  default = simple
[rerere]
  enabled = true
[sequence]
  editor = gvim -f
[user]
  email = anna.luisa.west@formidablelabs.com
  name = "Anna Luisa Pati√±o West"
[alias]
  s = status
  # branches
  ch = checkout
  br = branch
  bra = branch --all
  # staging & commits
  a = add
  re = reset
  com = commit
  ca = commit --amend
  can = commit --amend -C HEAD
  # diff
  d = diff
  dc = diff --cached
  ds = diff --stat
  # history
  l = log --format=\"%C(blue)%h%Creset %C(green)%s% %C(yellow)%d%Creset%n  %ad %an %n\" --date=relative
  lf = log --format=\"%n%C(blue)%h%Creset %C(green)%s%Creset %C(yellow)%d%Creset %n  %ad %an\" --date=relative --stat --name-status
  # fetch, pull, rebase
  f = fetch
  p = pull
  pr = pull --rebase
  r = rebase
  ri = rebase -i
  rc = rebase --continue
  # stash
  sa = "!sh -c 'git stash apply stash@{$1}' -"
  sd = "!sh -c 'git stash drop stash@{$1}' -"
  sl = stash list --format=\"\n%C(yellow)%gd%Creset %C(green)%s%Creset %ar\" --stat --name-status
  ss = stash save
  sp = stash pop
  shows = stash show -p
  # infrequently used
  bi = bisect
  cp = cherry-pick
  sup = submodule update
