[user]
  email = aisapatino@gmail.com
[color]
  ui = always
[diff]
  tool = gvimdiff
[difftool]
  prompt = false
[merge]
  tool = fugitive
[mergetool]
  prompt = false
  [mergetool.fugitive]
	cmd = gvim -f -c \"Gdiff\" \"$MERGED\"
[push]
  default = matching
[credential]
  helper = cache --timeout=3600
[alias]
  st = status
  ch = checkout
[core]
  autocrlf = false
  excludesfile = ~/.gitignore
  editor = gvim
