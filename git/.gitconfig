[advice]
  statusHints = false
[blame]
  date = short
[core]
  autocrlf = false
  excludesfile = ~/.gitignore
  editor = vim
[credential]
  helper = cache --timeout=3600
[diff]
  renames = true
  tool = gvimdiff
[difftool]
  prompt = false
[fetch]
  prune = true
[hub]
  host = gecgithub01.walmart.com
[merge]
  tool = fugitive
[mergetool]
  prompt = false
  keepBackup = false
[mergetool "fugitive"]
  cmd = gvim -f -c \"Gdiff\" \"$MERGED\"
[push]
  default = simple
[rebase]
  autoSquash = true
[rerere]
  enabled = true
[sequence]
  editor = gvim -f
[user]
  email = anna.luisa.west@formidablelabs.com
  name = "Anna Luisa Pati√±o West"
[alias]
  s = status
  # branches/remotes
  ch = checkout
  br = branch
  bra = branch --all
  mote = remote -v
  # staging & commits
  a = add
  re = reset
  cl = clean
  com = commit --verbose
  ca = commit --amend
  can = commit --amend -C HEAD
  cs = "!sh -c 'git commit --squash=$1' -"
  cf = "!sh -c 'git commit --fixup=$1' -"
  # diff
  d = diff
  dc = diff --cached
  ds = diff --stat
  # history
  l = log --format=\"%C(blue)%h %Creset%cd %C(green)%s%C(yellow)%d%Creset\" --date=format:%m/%d
  ll = log --format=\"%<(13,trunc)%an %C(blue)%h%Creset %cd %C(green)%s%C(yellow)%d\" --date=format:%m/%d
  lf = log --format=\"%n%C(magenta)%cd %C(green)%s%C(yellow)%d%n      %C(blue)%h %an\" --date=format:%m/%d --name-only
  # fetch, pull, rebase
  f = fetch --all
  p = pull
  pr = pull --rebase
  r = rebase
  ri = rebase -i
  rc = rebase --continue
  # stash
  sa = "!sh -c 'git stash apply stash@{$1}' -"
  sd = "!sh -c 'git stash drop stash@{$1}' -"
  sl = stash list --format=\"\n%C(yellow)%gd %C(green)%s%Creset %ar\" --name-status
  ss = stash save
  sp = stash pop
  shows = stash show -p
  # misc
  bi = bisect
  cp = cherry-pick
