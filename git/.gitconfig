[advice]
  statusHints = false
[blame]
  date = short
[core]
  autocrlf = false
  excludesfile = ~/.gitignore
  editor = vim
[credential]
  helper = cache --timeout=3600
[diff]
  renames = true
  tool = gvimdiff
[difftool]
  prompt = false
[fetch]
  prune = true
[hub]
  host = gecgithub01.walmart.com
[mailmap]
  file = ~/Projects/.mailmap
[merge]
  tool = fugitive
[mergetool]
  prompt = false
  keepBackup = false
[mergetool "fugitive"]
  cmd = gvim -f -c \"Gdiff\" \"$MERGED\"
[push]
  default = simple
[rebase]
  autoSquash = true
[rerere]
  enabled = true
[sequence]
  editor = gvim -f
[user]
  email = anna.luisa.west@formidablelabs.com
  name = "Anna Luisa Pati√±o West"
[alias]
  s = status
  # branches/remotes
  ch = checkout
  br = branch
  bra = branch --all
  mote = remote -v
  # staging & commits
  a = add
  re = reset
  cl = clean
  com = commit --verbose
  ca = commit --amend --verbose
  can = commit --amend -C HEAD
  cs = "!sh -c 'git commit --squash=$1' -"
  cf = "!sh -c 'git commit --fixup=$1' -"
  # diff
  d = diff
  dc = diff --cached
  ds = diff --stat
  # log
  l = log --format=\"%C(blue)%h %C(cyan)%cd %Creset%s%C(green)%d\" --date=format:%m/%d
  ll = log --format=\"%<(4,trunc)%aN %C(blue)%h %C(cyan)%cd %Creset%s%C(green)%d\" --date=format:%m/%d
  ln = log --format=\"%C(cyan)%cd %C(reset)%s%C(green)%d%n%w(80,6,6)%-b\" --date=format:%m/%d
  lf = log --format=\"%n%C(blue)%h %C(cyan)%cd %C(blue)%an%C(green)%d%n%n%s\" --date=format:%m/%d --name-only
  # fetch, pull, rebase
  f = fetch --all
  p = pull
  pr = pull --rebase
  r = rebase
  ri = rebase -i
  rc = rebase --continue
  # stash
  sa = "!sh -c 'git stash apply stash@{$1}' -"
  sd = "!sh -c 'git stash drop stash@{$1}' -"
  sl = stash list --format=\"\n%C(yellow)%gd %C(green)%s%Creset %ar\" --name-status
  ss = stash save
  sp = stash pop
  shows = stash show -p
  # misc
  tagl = tag --sort=-version:refname --format='%(creatordate:short)   %(color:green)%(align:width=10,position=left)%(refname:strip=2)%(end)%(color:reset)%(align:position=left,width=25)%(taggername)%(end)%(align:width=12,position=left)%(color:blue)%(*objectname:short)%(end)%(color:reset)%(contents:subject)'
  bi = bisect
  cp = cherry-pick
